{
  "name": "afrittella/laravel-loggable",
  "description": "Simple log implementation for Eloquent Models (Laravel 5.4)",
  "require": {
    "php": "~7.0",
    "illuminate/auth": "~5.4.0",
    "illuminate/container": "~5.4.0",
    "illuminate/contracts": "~5.4.0",
    "illuminate/database": "~5.4.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Andrea Frittella",
      "email": "a.frittella@sintab.it",
      "role": "Chief Architect & Developer",
      "homepage": "http://www.andreafrittella.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Afrittella\\LaravelLoggable\\": "src/app"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.7",
    "fzaninotto/faker": "~1.4",
    "orchestra/testbench": "^3.4",
    "orchestra/database": "*",
    "friendsofphp/php-cs-fixer": "^1.11"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Afrittella\\LaravelLoggable\\LoggableServiceProvider"
      ]
    }
  }
}
